{
  "name": "funarcade",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node backend/dist/server.js",
    "build": "npm run build:backend && npm run copy-env",
    "build:backend": "cd backend && npm install && tsc",
    "build:frontend": "cd frontend && npm install && npm run build",
    "copy-env": "node -e \"require('fs').copyFileSync('backend/.env', 'backend/dist/.env')\"",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run start",
    "clean": "rimraf backend/dist",
    "vercel-build": "npm run build"
  },
